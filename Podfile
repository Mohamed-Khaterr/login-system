# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'swift-login-system' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Firebase SDK
  pod 'FirebaseAuth'
  pod 'FirebaseFirestore'


  # Facebook SDK
  pod 'FacebookCore'
  pod 'FacebookLogin'
  
end
